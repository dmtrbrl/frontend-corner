{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "pnpm run frontend:build",
  "devCommand": "pnpm run frontend:dev",
  "outputDirectory": "frontend/.next",
  "framework": "nextjs",
  "ignoreCommand": "git diff --quiet $VERCEL_GIT_COMMIT_REF^ $VERCEL_GIT_COMMIT_SHA -- . ':data/' ':frontend/' ':shared/'"
}